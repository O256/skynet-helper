thread = 2
harbor = 0

-- 实际运行模式
-- logger = "./logs/game.log"
-- daemon = "./game.pid"

-- VSCode调试模式
logger = "vscdebuglog"
logservice = "snlua"
logmodel = 1 -- 日志模式, 0: 通用模式, 1: 折叠模式,只打印第一行,需要展开查看详细内容

start = "debug"
bootstrap = "snlua bootstrap"	-- The service for bootstrap

-- 调试端口
debug_console_port = 9201

luaservice = "./service/?.lua;./examples/?.lua"
lualoader = "lualib/loader.lua"
cpath = "./cservice/?.so"
